<%
from bottle import request
import utils
import datetime
import json

db = utils.ConnectDB()

query = '%' + request.params.get('query','') + '%'

db.execute("""SELECT idmat, descripcion as title
              FROM materiales
              WHERE descripcion like %s""",(query,))

list = []
for row in db:
    newrow = {}
    newrow['title'] = row['descripcion']
    newrow['url'] = "javascript:assign(id)".format(id=row['idmat']);
    list.append(newrow)
end


out = {'items': list}
%>
{{!json.dumps(out)}}